  "version": "1.0.3",
  "description": "An api implementation for reverb.com",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "author": "Zachary Eggert <Eggert.Zachary@gmail.com>",
  "license": "MIT",
  "keywords": [
    "reverb",
